<%= content_tag_for :li, videosfeed do %>
	<div class="row-fluid">
		<div class="span1"><div class="handle"></div></div>
		<div class="span8 list-left">
	  		<h4><%= videosfeed.title %></h4>
	  		<p><%= videosfeed.youtube_username %></p>
	  	</div>
	  	<div class="span3 list-controls">
	  		<div class="current-actions-wrap">
	  			<div class="current-actions">
			  		<div class="btn-group">
			  		<%= link_to raw('<i class="icon-edit"></i> Edit'), edit_user_videosfeed_path(@user, videosfeed), :remote => true, :class => 'btn btn-small' %>
			  		<%= link_to raw('<i class="icon-trash"></i> Delete'), [@user, videosfeed], :confirm => 'Are you sure?', :method => :delete, :remote => true, :class => 'btn btn-small' %>
			  		</div>
			  	</div>
			</div>
		</div>
	</div>
	<div id="inner_<%= dom_id(videosfeed) %>" class="row-fluid listItemInner"></div>
<% end %>