<div class="box-model-header"><h1>Testing</h1></div>         
  <div class="box-model-content">
    <div class="row-fluid">
      <div class="span12 guide">
        <div class="count">
          <h3>Testers</h3>
          <div id="count" class="feed-count"><%= @user.testers.count %></div>
        </div>
        <%= render "shared/mini_guide" %>
      </div>
    </div>
    <div class="row-fluid">
      <div class="span12">
        <div class="addnew">
          <div id="testerForm">
            <%= render 'form' %>
          </div>
        </div>
      </div>
    </div>
    <div class="row-fluid">
      <div class="span12">
        <ul id="testerList" class="showList"><%= render @testers %></ul>
      </div>
    </div>
  </div>
</div>