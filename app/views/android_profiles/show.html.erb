<div class="box-model-header"><h1>Android Marketplace Profile</h1></div>   
<div class="box-model-content">
  <div class="row-fluid">
    <div class="span12 guide">
      <div class="count">
        <h3>Ready</h3>
        <div id="count" class="feed-count">&#x2713;</div>
      </div>
      <%= render "shared/mini_guide" %>
    </div>
  </div>
  <div class="row-fluid">
    <div class="span12">
      <ul class="showList">
      	<li>
      		<div class="row-fluid">
    				<div class="span1"><div class="cms-icon cms-directory1"></div></div>
    				<div class="span8 list-left">
    			  		<h4><%= @android_profile.username %></h4>
    			  		<p><%= @android_profile.pass %></p>
    			  	</div>
    		  	<div class="span3 list-controls">
    		  		<div class="current-actions-wrap">
    		  			<div class="current-actions">
    				  	<div class="btn-group">
    				  	<%= link_to raw('<i class="icon-edit"></i> Edit'), edit_user_android_profile_path, :class => 'btn btn-small' %>
          				<%= link_to raw('<i class="icon-trash"></i> Delete'), user_android_profile_path(@user), :confirm => 'Are you sure?', :method => :delete, :class => 'btn btn-small' %>
    				  	</div>
    				  	</div>
    				  </div>
    				</div>
    			</div>
      	</li>
      </ul>
    </div>
  </div>
  <br/>
  <div class="row-fluid">
    <div class="span12">
    </div>
  </div>
</div>