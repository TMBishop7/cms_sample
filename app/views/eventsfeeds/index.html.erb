<div class="box-model-header"><h1>Events</h1></div>           
  <div class="box-model-content">
    <div class="row-fluid">
      <div class="span12 guide">
        <div class="count">
          <h3>Feeds</h3>
          <div id="count" class="feed-count"><%= @eventsfeeds.count %></div>
        </div>
        <%= render "shared/mini_guide" %>
      </div>
    </div>
    <div class="row-fluid">
      <div class="span12">
        <div class="addnew">
          <div id="eventsfeed_form"> <!--HERE-->
            <%= render 'form' %>
          </div>
            <script>
              function createPicker() { 
                $("#eventsfeed_color").spectrum({ preferredFormat: "hex", showInput: true });
              }
              createPicker(); 
            </script>
        </div>
      </div>
    </div>
    <div class="row-fluid">
      <div class="span12">
        <ul id="feedsEvents" class="showList" data-update-url="<%= sort_user_eventsfeeds_url %>"><%= render :partial => @eventsfeeds %></ul> <!--HERE-->
      </div>
    </div>
  </div>
</div>
