<%= content_tag_for :li, eventsfeed do %>
	<div class="row-fluid">
		<div class="span1"><div class="handle"></div></div>
		<div class="span1"><div class="iconpreview" style="background-color:<%= eventsfeed.color %>;"></div></div>
		<div class="span7 list-left">
	  		<h4><%= eventsfeed.title %></h4>
	  		<p><%= eventsfeed.ical_url %></p>
	  	</div>
	  	<div class="span3 list-controls">
	  		<div class="current-actions-wrap">
	  			<div class="current-actions">
			  		<div class="btn-group">
			  		<%= link_to raw('<i class="icon-edit"></i> Edit'), edit_user_eventsfeed_path(@user, eventsfeed), :remote => true, :class => 'btn btn-small' %>
			  		<%= link_to raw('<i class="icon-trash"></i> Delete'), [@user, eventsfeed], :confirm => 'Are you sure?', :method => :delete, :remote => true, :class => 'btn btn-small' %>
			  		</div>
			  	</div>
			</div>
		</div>
	</div>
	<div id="inner_<%= dom_id(eventsfeed) %>" class="row-fluid listItemInner"></div>
<% end %>