  <div class="row-fluid">
    <div class="span12 guide">
      <div class="count">
        <h3>Sizes</h3>
        <div id="count" class="feed-count"><%= @mobile_web_feature_icons.count %></div>
      </div>
      <%= render "shared/mini_guide" %>
    </div>
  </div>
  <div class="row-fluid">
      <div class="span12">
        <div class="addnew">
          <div id="mobileWebFeatureIcon_form">
            <%= render 'form' %>
          </div>
        </div>
      </div>
    </div>
  <div class="row-fluid">
    <div class="span12">
      <ul id="mobileWebFeatureList" class="showList">
        <%= render @mobile_web_feature_icons %>
      </ul>
    </div>
  </div>
</div>